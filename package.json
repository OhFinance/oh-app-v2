{
  "name": "@ohfinance/oh-app",
  "description": "Oh! Finance App",
  "version": "0.2.1",
  "license": "MIT",
  "author": "Oh! Finance <hello@oh.finance>",
  "private": true,
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "volta": {
    "node": "14.17.6",
    "yarn": "1.22.11"
  },
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build && node ./scripts/generate-sitemap",
    "export": "npm run build && next export -o _static",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir src --dir scripts",
    "format": "npx prettier src scripts ./*.js ./*.json --check",
    "format:fix": "npx prettier src scripts ./*.js ./*.json --write",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.5.2",
    "@gnosis.pm/safe-apps-web3-react": "^0.6.5",
    "@popperjs/core": "^2.10.2",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/lodash": "^4.14.177",
    "@web3-react/core": "^6.1.9",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/portis-connector": "^6.1.9",
    "@web3-react/walletconnect-connector": "^6.2.8",
    "@web3-react/walletlink-connector": "^6.2.8",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.1",
    "d3-dsv": "^3.0.1",
    "d3-time-format": "^4.0.0",
    "ethers": "^5.5.1",
    "md5": "^2.3.0",
    "next": "12.0.4",
    "next-compose-plugins": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-popper": "^2.2.5",
    "react-redux": "^7.2.6",
    "recharts": "^2.1.6",
    "redux-localstorage-simple": "^2.4.1",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.2",
    "ua-parser-js": "^1.0.2",
    "web3": "^1.6.1",
    "web3-core": "^1.6.1",
    "web3-eth-contract": "^1.6.1",
    "web3-utils": "^1.6.1",
    "zustand": "^3.6.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.4",
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@types/d3-dsv": "^3.0.0",
    "@types/d3-time-format": "^4.0.0",
    "@types/md5": "^2.3.1",
    "@types/react": "17.0.36",
    "@types/recharts": "^1.8.23",
    "@types/tailwindcss": "^2.2.4",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cross-env": "^7.0.3",
    "eslint": "<8.0.0",
    "eslint-config-next": "^12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.1",
    "prettier": "^2.4.1",
    "trash-cli": "5.0.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}

{
  "name": "@ohfinance/oh-app",
  "description": "Oh! Finance App",
  "version": "0.2.1",
  "license": "MIT",
  "author": "Oh! Finance <hello@oh.finance>",
  "private": true,
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "volta": {
    "node": "14.18.1",
    "yarn": "1.22.11"
  },
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "export": "npm run build && next export -o _static",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir src --dir scripts",
    "format": "npx prettier src scripts ./*.js ./*.json --check",
    "format:fix": "npx prettier src scripts ./*.js ./*.json --write",
    "prepare": "npx husky install",
    "contracts:compile:abi": "typechain --target ethers-v5 --out-dir src/abis/types \"./src/abis/**/*.json\""
  },
  "dependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/address": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "@metamask/jazzicon": "^2.0.0",
    "@popperjs/core": "^2.10.2",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/lodash": "^4.14.177",
    "@uniswap/v2-sdk": "^3.0.1",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.8",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.1",
    "d3-dsv": "^3.0.1",
    "d3-time-format": "^4.0.0",
    "ethers": "^5.5.1",
    "lightweight-charts": "^3.8.0",
    "lodash": "^4.17.21",
    "next": "12.0.4",
    "next-compose-plugins": "^2.2.1",
    "polished": "^4.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-icons": "^4.4.0",
    "react-popper": "^2.2.5",
    "react-redux": "^7.2.6",
    "react-tippy": "^1.4.0",
    "react-spinners": "^0.13.4",
    "rebass": "^4.0.7",
    "recharts": "^2.1.6",
    "redux-localstorage-simple": "^2.4.1",
    "tailwindcss": "^2.2.19",
    "zustand": "^3.6.5"
  },
  "devDependencies": {
    "@gnosis.pm/safe-apps-web3-react": "^0.6.7",
    "@next/bundle-analyzer": "^12.0.4",
    "@reach/dialog": "^0.16.2",
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@typechain/ethers-v5": "^8.0.5",
    "@types/d3-dsv": "^3.0.0",
    "@types/d3-time-format": "^4.0.0",
    "@types/ms.macro": "^2.0.0",
    "@types/react": "17.0.36",
    "@types/rebass": "^4.0.10",
    "@types/recharts": "^1.8.23",
    "@types/styled-components": "^5.1.24",
    "@types/ua-parser-js": "^0.7.36",
    "@uniswap/sdk-core": "^3.0.1",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/portis-connector": "^6.1.9",
    "@web3-react/walletlink-connector": "^6.2.8",
    "cross-env": "^7.0.3",
    "eslint": "<8.0.0",
    "eslint-config-next": "^12.0.4",
    "file-loader": "^6.2.0",
    "fortmatic": "^2.2.1",
    "globby": "^12.0.2",
    "husky": "^7.0.4",
    "inspectpack": "^4.7.1",
    "jsbi": "^3.1.4",
    "lint-staged": "^12.1.1",
    "ms.macro": "^2.0.0",
    "next-transpile-modules": "^9.0.0",
    "prettier": "^2.4.1",
    "react-spring": "^9.4.4",
    "react-use-gesture": "^9.1.3",
    "styled-components": "^5.3.5",
    "typechain": "^6.0.5",
    "typescript": "^4.5.2",
    "ua-parser-js": "^1.0.2"
  },
  "resolutions": {
    "buffer": "5.7.1",
    "eventemitter3": "4.0.7",
    "lodash": "4.17.21",
    "bn.js": "5.2.0",
    "react-is": "17.0.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
